TABBY_API_BASE_URL=https://api.tabby.cash

# Auth (pick one):
# - Moltbook: set MOLTBOOK_API_KEY (+ optional MOLTBOOK_AUDIENCE)
# - Dev auth (server ENFORCE_MOLTBOOK=false): set TABBY_DEV_AUTH_TOKEN to match server DEV_AUTH_TOKEN
MOLTBOOK_API_KEY=moltbook_xxx
MOLTBOOK_AUDIENCE=tabby.local
TABBY_DEV_AUTH_TOKEN=

# Used for onchain transactions (optional; if omitted, the CLI will use /public/config and defaults).
MONAD_CHAIN_ID=143
MONAD_RPC_URL=https://rpc.monad.xyz

# Gas automation defaults (wei)
# - TABBY_MIN_TX_GAS_WEI: if wallet balance drops below this, auto-topup will request a gas-loan
# - TABBY_GAS_TOPUP_WEI: requested principal when topping up
TABBY_MIN_TX_GAS_WEI=10000000000000000
TABBY_GAS_TOPUP_WEI=20000000000000000
TABBY_GAS_TOPUP_INTEREST_BPS=500
TABBY_GAS_TOPUP_DURATION_SECONDS=3600
TABBY_GAS_TOPUP_ACTION=1

# Optional override (if you want the CLI to work without calling /public/config)
AGENT_LOAN_MANAGER_ADDRESS=0x0000000000000000000000000000000000000000

# Secured loans (optional overrides; LOAN_MANAGER_ADDRESS is enough, others can be discovered)
LOAN_MANAGER_ADDRESS=0x0000000000000000000000000000000000000000
POSITION_MANAGER_ADDRESS=0x0000000000000000000000000000000000000000
SECURED_POOL_ADDRESS=0x0000000000000000000000000000000000000000
COLLATERAL_ASSET=0x0000000000000000000000000000000000000000
